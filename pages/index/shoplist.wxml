<view class="box">
<block wx:for="{{shop}}" wx:key="{{index}}">
  <view class="shopbox"  bindtap="jump">
  <image src='{{item.url}}' style="width:100%;height:300rpx"></image>
  <view class="shopInformation">
  <text class="mainTitle">{{item.mainTitle}}</text>
  <text class="subTitle">{{item.subTitle}}</text>
  <text class="money">￥{{item.money}}</text>
  <text class="evaluate">{{item.evaluate}}人评价</text>
  <text class="delivery">{{item.delivery}}</text>
  </view>
  </view>
  </block>
</view>
